{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "actuallymab/laravel-comment": "0.5.0",
        "algolia/algoliasearch-client-php": "1.25.1",
        "barryvdh/laravel-ide-helper": "v2.4.3",
        "etdsolutions/microplugin": "0.0.3",
        "fideloper/proxy": "~4.0",
        "gloudemans/shoppingcart": "2.5.0",
        "illuminate/database": "5.6.x-dev",
        "laravel/framework": "5.6.*",
        "laravel/scout": "v4.0.3",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "5.6.x-dev",
        "venturecraft/revisionable": "1.28.0",
        "matriphe/imageupload": "6.*"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "matriphe/imageupload": "6.1.2",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "~1.1",
        "phpunit/phpunit": "~7.0",
        "riverskies/laravel-mobile-detect": "1.3",
        "symfony/thanks": "^1.0",
        "vanilo/order": "0.2.2"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "venturecraft/revisionable": "1.*",
    "minimum-stability": "dev",
    "prefer-stable": true
}
